    649  f305		       b0 fc		      BCS	.divideby15	; 06/07 - 11/16/21/26/31/36/41/46/51/56/61/66
    650  f307
    651  f307		       a8		      TAY		; 08 ; At this point the value in A is -1 to -15. In this code I use a table
    652  f308							; to quickly convert that value to the fine adjust value needed.
    653  f308		       b9 0f f9 	      LDA	fineAdjustTable,Y	; 13 -> Consume 5 cycles by guaranteeing we cross a page boundary
    654  f30b							; In your own code you may wish to consume only 4.
    655  f30b		       95 20		      STA	HMP0,X	; 17 Store the fine adjustment value.
    656  f30d		       95 10		      STA	RESP0,X	; 21/ 26/31/36/41/46/51/56/61/66/71 - Set the rough position.
    657  f30f
    658  f30f		       85 02		      STA	WSYNC
